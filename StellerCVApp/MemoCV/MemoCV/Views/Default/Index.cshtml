
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Start your development with Steller landing page.">
    <meta name="author" content="Devcrud">
    <title>Steller Landing page | Free Bootstrap 4.1 landing page</title>
    <!-- font icons -->
    <link rel="stylesheet" href="~/steller/public_html/assets/vendors/themify-icons/css/themify-icons.css">
    <!-- Bootstrap + Steller main styles -->
	<link rel="stylesheet" href="~/steller/public_html/assets/css/steller.css">
</head>
<body data-spy="scroll" data-target=".navbar" data-offset="40" id="home">

    <!-- Page navigation -->
    @await Html.PartialAsync("NavbarPartial")
    <!-- End of page navibation -->

    <!-- Page Header -->
    @await Component.InvokeAsync("FeatureList")
    <!-- End of Page Header -->
    
    <!-- About section -->
    @await Component.InvokeAsync("AboutList")

    <!-- Service section -->
    @await Component.InvokeAsync("ServiceList")
    <!-- End of Service sections -->

    <!-- Skills section -->
    @await Component.InvokeAsync("SkillList")
    <!-- End of Skills sections -->

    <!-- Testmonial Section -->
    @await Component.InvokeAsync("TestimonialList")
    <!-- End of testmonial section -->
    

    <!-- Contact Section -->
    <section id="contact" class="position-relative section">
        <div class="container text-center">
            <h6 class="subtitle">İletişim</h6>
            <h6 class="section-title mb-4">Benimle İrtibata Geçiniz.</h6>
            <div class="contact text-left">
                @* @await Component.InvokeAsync("SendMessage") *@
                @await Html.PartialAsync("/Views/Default/SendMessage.cshtml")
                @await Component.InvokeAsync("ContactDetails")                
            </div>
        </div>  
        <div id="map">
            <iframe src="https://snazzymaps.com/embed/61257"></iframe>
        </div>      
    </section>
    <!-- End of Contact Section -->

    <!-- Page Footer -->
    <footer class="page-footer">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-sm-6">
                    <p>Copyright <script>document.write(new Date().getFullYear())</script> &copy; <a href="http://www.devcrud.com" target="_blank">DevCRUD</a></p>
                </div>               
            </div>
        </div>
    </footer> 
    <!-- End of page footer -->
	
	<!-- core  -->
    <script src="~/steller/public_html/assets/vendors/jquery/jquery-3.4.1.js"></script>
    <script src="~/steller/public_html/assets/vendors/bootstrap/bootstrap.bundle.js"></script>
    <!-- bootstrap 3 affix -->
	<script src="~/steller/public_html/assets/vendors/bootstrap/bootstrap.affix.js"></script>

    <!-- steller js -->
    <script src="~/steller/public_html/assets/js/steller.js"></script>

</body>
</html>
